command /등록낚시특수어종:
    permission: op.*
    permission message: &4권한이 없습니다.
    trigger:
        if player is op:

            send "  {@w} &a특수 어종 등록을 시작합니다!"

            # 중복 등록 방지를 위한 파일 검토:
            # __Server_Data__/Main_Items/_cache/fish_registed.yml
            if Filecheking("%{@sub}%", "fish_registed", "yml") is true:

                send ""
                send "  {@w} &7중복 등록 방지가 작동하여 등록을 취소합니다."
                send "  {@w} &7특수 어종을 재등록해야한다면,"
                send "  {@w} &e__Server_Data__/Main_Items/_cache/fish_registed.yml"
                send "  {@w} &c파일을 삭제 후 재시도 하여주시기 바랍니다."

                stop

            else:

                # 중복 등록 방지 파일 생성
                cYML("%{@sub}%/fish_registed.yml", "value: register_name:%player%||value: uuid:%uuid of player%||value: time: %now%")

