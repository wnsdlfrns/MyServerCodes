options:
    worlds : "Survival1" or "Survival2" or "jawonworld"

    main : "__Server_Data__/Main_Items"
    sub : "__Server_Data__/Main_Items/_cache"

    cmain : "__Server_Data__/Users/_Fishing"

    t : &f&l[&6&l내 서버&f&l-&b&l낚시&f&l]&r
    w : &c&l(&r&e!&c&l)&r
    lt : &7&l>&r

    use.collections : true

    b_rivers : frozen river or river
    b_lake : taiga or sunflower plains or dark forest or snowy taiga or plains
    b_ocean : beach or lukewarm ocean or cold ocean or frozen ocean or ocean or warm ocean
    b_docean : deep ocean or deep cold ocean or deep frozen ocean or deep lukewarm ocean

    # 무게 추가 확률 (1~5 번호가 올라가는 순으로 더 희귀한 무게!)
    chance.1 : 50%
    chance.2 : 30%
    chance.3 : 20%
    chance.4 : 10%

    # 5와 6은 임시 공백

    # 넵튜니움 드랍률
    chance.7 : 3%
    # [세계의 빛] 태그용
    chance.7.star : 3

    # 8과 9는 임시 공백

    # 잉어 대
    chance.10 : 5%

    # 참다랑어 특대A - 바다일반
    chance.11 : 1%

    # 참다랑어 대A - 바다일반
    chance.12 : 3%

    # 참다랑어 특대B - 바다깊은
    chance.13 : 3%

    # 참다랑어 대B - 바다깊은
    chance.14 : 6%

    # 분홍연어 특대
    chance.15 : 1%

    # 분홍연어 대
    chance.16 : 10%

    # 문어 특대
    chance.17 : 1%

    # 문어 대
    chance.18 : 7%

    # 이벤트 관련
    event.run.ch : 40%

    event.get.1 : 10%
    event.get.2 : 25%
    event.get.3 : 15%
    event.get.4 : 15%
    event.get.5 : 10%
    event.get.6 : 5%





on command:
    if {fising.event.cmd.%uuid of event-player%} is set:
        if player is op:
            delete {fising.event.cmd.%uuid of event-player%}
            stop
        if player has permission "md.*":
            send "  {@w} &7MD의 권한으로 명령어 묵인 이벤트가 삭제됩니다." to event-player
            delete {fising.event.cmd.%uuid of event-player%}
            stop

        cancel event

        set {_co} to {fising.event.cmd.%uuid of event-player%}

        send "  {@w} &7아무 블럭이나 &f%{_co}%&f번 &7채집하거나 파괴하세요." to event-player
        stop



on break:
    if {fising.event.cmd.%uuid of event-player%} is set:
        if {fising.event.cmd.%uuid of event-player%} is 1:
            delete {fising.event.cmd.%uuid of event-player%}

        if {fising.event.cmd.%uuid of event-player%} is set:
            subtract 1 from {fising.event.cmd.%uuid of event-player%}



on death:
    if {fising.event.cmd.%uuid of event-player%} is set:
        delete {fising.event.cmd.%uuid of event-player%}
        if player's money >= 100000:
            remove 100000 from player's money
            send "  {@w} &e당신은 낚시 패널티를 받고 죽었습니다!"
            send "  {@w} &7벌금으로 &c100,000원&7이 차감되었습니다."




on fishing:
    if event-player's world is {@worlds}:

        # 아이템 드랍 시 플레이어 발위치 좌표 + y 0.5 에 소환되게끔.
        set {_loc} to player's location
        add 0.5 to y coord of {_loc}

        # 바이옴 인식
        # 강
        if biome of event-player's location is {@b_rivers}:
            set {fising.biome.%uuid of event-player%} to "river"

        if biome of event-player's location is {@b_lake}:
            set {fising.biome.%uuid of event-player%} to "lake"

        if biome of event-player's location is {@b_ocean}:
            set {fising.biome.%uuid of player%} to "ocean"

        if biome of event-player's location is {@b_docean}:
            set {fising.biome.%uuid of event-player%} to "deep ocean"



        if "%event.getState().name()%" is "BITE":

            set {_il::*} to uncolored lore of event-player's tool
            set {_esf} to enchant level of Luck of the Sea of player's tool
            if {_esf} is not set:
                set {_esf} to 0

            loop {_il::*}:
                if loop-value contains "[일반 도구]":
                    set {_true} to true
                    set {fising.state.%uuid of event-player%} to 1
                    chance of {@event.run.ch}:
                        set {fising.event.%uuid of event-player%} to true
                    stop loop

                if loop-value contains "[보석의 빛]":
                    set {_true} to true
                    set {fising.state.%uuid of event-player%} to 2
                    chance of {@event.run.ch}:
                        set {fising.event.%uuid of event-player%} to true
                    stop loop

                if loop-value contains "[육중한 무게]":
                    set {_true} to true
                    set {fising.state.%uuid of event-player%} to 3
                    chance of {@event.run.ch}:
                        set {fising.event.%uuid of event-player%} to true
                    stop loop

                if loop-value contains "[바다의 보물]":
                    set {_true} to true
                    set {fising.state.%uuid of event-player%} to 4
                    chance of {@event.run.ch}:
                        set {fising.event.%uuid of event-player%} to true
                    stop loop

                if loop-value contains "[세계의 빛]":
                    set {_true} to true
                    set {fising.state.%uuid of event-player%} to 5
                    set {fising.state.esf.%uuid of event-player%} to {_esf}
                    chance of {@event.run.ch}:
                        set {fising.event.%uuid of event-player%} to true
                    stop loop

            if {_true} is true:
                stop

            else:
                set {fising.state.%uuid of event-player%} to 0


        if "%event.getState().name()%" is "CAUGHT_FISH":


            # 도감용 YML 데이터 콜링
            if {@use.collections} is true:
                if {fising.state.%uuid of event-player%} is not 1:

                    if Filecheking("%{@cmain}%", "%uuid of player%", "yml") is not set:
                        cYML("%{@cmain}%/%uuid of player%.yml", "value: name:%player%||value: uuid:%uuid of player%")

                    set {_d} to "%{@cmain}%/%uuid of player%.yml"
                    load yaml {_d} as {_d}
            #


            # 랜덤 이벤트 작동
            if {fising.event.%uuid of event-player%} is true:
                delete {fising.event.%uuid of event-player%}
                set {_e} to random integer between 1 and 6
                if {_e} is 1:
                    chance of {@event.get.1}:
                        set {fising.event.%uuid of event-player%} to 1
                if {_e} is 2:
                    chance of {@event.get.2}:
                        set {fising.event.%uuid of event-player%} to 2
                if {_e} is 3:
                    chance of {@event.get.3}:
                        set {fising.event.%uuid of event-player%} to 3
                if {_e} is 4:
                    chance of {@event.get.4}:
                        set {fising.event.%uuid of event-player%} to 4
                if {_e} is 5:
                    chance of {@event.get.5}:
                        set {fising.event.%uuid of event-player%} to 5
                if {_e} is 6:
                    chance of {@event.get.5}:
                        set {fising.event.%uuid of event-player%} to 6


            # 일반 낚시대
            if {fising.state.%uuid of event-player%} is 0:
                stop

            # [일반 도구]: 내구만 조금 많은 일반적인 낚싯대
            # " 마인크래프트 오리지널 어종 " / 강, 호수, 바다 상관 없이 오리지널 어종이 낚임
            if {fising.state.%uuid of event-player%} is 1:
                stop

            event.getCaught().remove()

            # [보석의 빛]
            # " 강, 호수 낚시 전용 및 오리지널 어종 + 특수 어종 "
            if {fising.state.%uuid of event-player%} is 2:

                # 이벤트 구간
                if {fising.event.%uuid of event-player%} is set:

                    # 귀엽고 깜찍한 수달 이벤트
                    if {fising.event.%uuid of event-player%} is 1:

                        send colored "{@w} {@t} <##fffcac>&l귀여운척하던 수달에게 물고기를 도둑맞았습니다." to event-player

                        delete {fising.event.%uuid of event-player%}
                        stop


                    # 문어 먹물 이벤트
                    if {fising.event.%uuid of event-player%} is 2:

                        send colored "{@w} {@t} &8&l문어가 먹물을 쏘고 도망갔다!!" to event-player
                        apply blindness 1 to event-player for 40 seconds

                        delete {fising.event.%uuid of event-player%}
                        

                    # 길 잃은 이벤트
                    if {fising.event.%uuid of event-player%} is 3:

                        send colored "{@w} {@t} &8&l...여기는 어디일까요....?" to event-player

                        execute console command "cmi rt %player% jawonworld"

                        set {fising.event.cmd.%uuid of event-player%} to 30

                        delete {fising.event.%uuid of event-player%}
                        stop


                    # 번개 이벤트
                    if {fising.event.%uuid of event-player%} is 4:

                        send colored "{@w} {@t} <##df0000>&l아악!!! 너무 아프다...." to event-player

                        create a safe explosion of force 3 at event-player
                        strike lightning at event-player
                        damage event-player by 8 hearts

                        delete {fising.event.%uuid of event-player%}
                        stop


                    # 독 이벤트
                    if {fising.event.%uuid of event-player%} is 5:

                        send colored "{@w} {@t} <##7eff00>&l같이 낚인 독포션에 감염되었다." to event-player
                        apply poison 2 to event-player for 30 seconds

                        delete {fising.event.%uuid of event-player%}


                    # VIP 티켓 이벤트
                    if {fising.event.%uuid of event-player%} is 6:

                        send colored "{@w} {@t} &7&l무언가 낚아 올렸다..." to event-player
                        drop 1 of paper named "&f&l[ &b물고기 &e&lVIP &b판매권 &f&l]" with lore "" and "&71회 판매에 한하여 물고기 가격의 2배를" and "&7받을 수 있습니다." and "" and "&a&l거래 가능 아이템" and "&c&l(시스템 보호 아이템)" at {_loc}
                        
                        delete {fising.event.%uuid of event-player%}
                        stop


                    delete {fising.event.%uuid of event-player%}



                # 강
                if {fising.biome.%uuid of event-player%} is "river":

                    set {_r} to random integer between 1 and 8

                    if {_r} is 1:
                        send action bar "&a&l+ 대구" to event-player
                        drop 1 of cod at {_loc}

                    if {_r} is 2:
                        send action bar "&a&l+ 연어" to event-player
                        drop 1 of salmon at {_loc}

                    if {_r} is 3:
                        send action bar "&a&l+ 열대어" to event-player
                        drop 1 of tropical fish at {_loc}

                    if {_r} is 4:
                        send action bar "&a&l+ 복어" to event-player
                        drop 1 of pufferfish at {_loc}

                    if {_r} is 5:
                        send action bar "&a&l+ 민물고기" to event-player
                        drop 1 of {fishing.fish.민물고기::1} with lore "" and "&8민물고기" and "&8[평범]" at {_loc}

                    if {_r} is 6:
                        send action bar "&a&l+ 민물가재" to event-player
                        drop 1 of {fishing.fish.민물가재::1} with lore "" and "&8민물가재" and "&8[평범]" at {_loc}

                    if {_r} is 7:
                        send action bar "&a&l+ 노가리" to event-player
                        drop 1 of {fishing.fish.노가리::1} with lore "" and "&8노가리" and "&8[평범]" at {_loc}

                    if {_r} is 8:
                        send action bar "&a&l+ 농성어" to event-player
                        drop 1 of {fishing.fish.농성어::1} with lore "" and "&8농성어" and "&8[평범]" at {_loc}

                    unload yaml {_d}

                    stop

                # 호수
                if {fising.biome.%uuid of event-player%} is "lake":

                    set {_r} to random integer between 1 and 9

                    if {_r} is 1:
                        send action bar "&a&l+ 대구" to event-player
                        drop 1 of cod at {_loc}

                    if {_r} is 2:
                        send action bar "&a&l+ 연어" to event-player
                        drop 1 of salmon at {_loc}

                    if {_r} is 3:
                        send action bar "&a&l+ 열대어" to event-player
                        drop 1 of tropical fish at {_loc}

                    if {_r} is 4:
                        send action bar "&a&l+ 복어" to event-player
                        drop 1 of pufferfish at {_loc}

                    if {_r} is 5:
                        send action bar "&a&l+ 민물고기" to event-player
                        drop 1 of {fishing.fish.민물고기::1} with lore "" and "&8민물고기" and "&8[평범]" at {_loc}

                    if {_r} is 6:
                        send action bar "&a&l+ 민물가재" to event-player
                        drop 1 of {fishing.fish.민물가재::1} with lore "" and "&8민물가재" and "&8[평범]" at {_loc}

                    if {_r} is 7:
                        send action bar "&a&l+ 농성어" to event-player
                        drop 1 of {fishing.fish.농성어::1} with lore "" and "&8농성어" and "&8[평범]" at {_loc}

                    if {_r} is 8:
                        send action bar "&a&l+ 틸라피아" to event-player
                        drop 1 of {fishing.fish.틸라피아::1} with lore "" and "&8틸라피아" and "&8[평범]" at {_loc}

                    if {_r} is 9:

                        # 대 (15 ~ 50 kg)
                        chance of {@chance.10}:

                            set {_kg} to random integer between 15 and 50
                            set {_g} to random integer between 1 and 9
                            if {_kg} >= 50:
                                set {_g} to 0

                            drop 1 of {fishing.fish.잉어::1} with lore "" and "{@lt} &a크기: &e대" and "{@lt} &d무게: &f%{_kg}%&f.&f%{_g}% &fkg" and "{@lt} &b어종: &f잉어" and "" and "&8[특별]" at {_loc}
                            send action bar "&a&l+ 잉어(대)" to event-player
                            broadcast "{@t} &b%event-player%&e님께서 &a잉어(대) &7%{_kg}%&7.&7%{_g}%&7kg &e를 낚으셨습니다!"

                            # 도감 기록 사용시 별도 작동
                            if {@use.collections} is true:
                                set yaml value "잉어_대" from {_d} to "%{_kg}%.%{_g}%"
                                save yaml {_d}
                                unload yaml {_d}

                            stop

                        drop 1 of {fishing.fish.잉어::1} with lore "" and "&8잉어" and "&8[평범]" at {_loc}
                        send action bar "&a&l+ 잉어" to event-player

                    unload yaml {_d}
                        
                    stop

                else:
                    send action bar "&e&l이 낚싯대는 여기에서 사용이 불가능해보인다." to event-player
                    stop


            # [육중한 무게]
            # " 바다-일반 낚시 전용 및 특수 어종 " / 특이사항: 제한된 확률로 넵튠 조각(1) 획득 (행운적용x)
            if {fising.state.%uuid of event-player%} is 3:


                # 이벤트 구간
                if {fising.event.%uuid of event-player%} is set:

                    # 귀엽고 깜찍한 수달 이벤트
                    if {fising.event.%uuid of event-player%} is 1:

                        send colored "{@w} {@t} <##fffcac>&l귀여운척하던 수달에게 물고기를 도둑맞았습니다." to event-player

                        delete {fising.event.%uuid of event-player%}
                        stop


                    # 문어 먹물 이벤트
                    if {fising.event.%uuid of event-player%} is 2:

                        send colored "{@w} {@t} &8&l문어가 먹물을 쏘고 도망갔다!!" to event-player
                        apply blindness 1 to event-player for 40 seconds

                        delete {fising.event.%uuid of event-player%}
                        

                    # 길 잃은 이벤트
                    if {fising.event.%uuid of event-player%} is 3:

                        send colored "{@w} {@t} &8&l...여기는 어디일까요....?" to event-player

                        execute console command "cmi rt %player% jawonworld"

                        set {fising.event.cmd.%uuid of event-player%} to 30

                        delete {fising.event.%uuid of event-player%}
                        stop


                    # 번개 이벤트
                    if {fising.event.%uuid of event-player%} is 4:

                        send colored "{@w} {@t} <##df0000>&l아악!!! 너무 아프다...." to event-player

                        create a safe explosion of force 3 at event-player
                        strike lightning at event-player
                        damage event-player by 8 hearts

                        delete {fising.event.%uuid of event-player%}
                        stop


                    # 독 이벤트
                    if {fising.event.%uuid of event-player%} is 5:

                        send colored "{@w} {@t} <##7eff00>&l같이 낚인 독포션에 감염되었다." to event-player
                        apply poison 2 to event-player for 30 seconds

                        delete {fising.event.%uuid of event-player%}


                    # VIP 티켓 이벤트
                    if {fising.event.%uuid of event-player%} is 6:

                        send colored "{@w} {@t} &7&l무언가 낚아 올렸다..." to event-player
                        drop 1 of paper named "&f&l[ &b물고기 &e&lVIP &b판매권 &f&l]" with lore "" and "&71회 판매에 한하여 물고기 가격의 2배를" and "&7받을 수 있습니다." and "" and "&a&l거래 가능 아이템" and "&c&l(시스템 보호 아이템)" at {_loc}
                        
                        delete {fising.event.%uuid of event-player%}
                        stop


                    delete {fising.event.%uuid of event-player%}



                # 바다-일반
                if {fising.biome.%uuid of event-player%} is "ocean":

                    set {_r} to random integer between 1 and 5

                    # 3% 확률로 넵튜니움 조각 드랍
                    chance of {@chance.7}:
                        drop 1 of {fishing.fish.넵튠조각::1} at {_loc}
                        play sound "item.trident.return" with volume 0.4 and pitch 2 for event-player

                    if {_r} is 1:

                        # 특대 (700 ~ 2000 kg)
                        chance of {@chance.11}:

                            set {_kg} to random integer between 700 and 800
                            set {_g} to random integer between 1 and 9


                            chance of {@chance.1}:
                                set {_kg} to random integer between 700 and 900

                            chance of {@chance.2}:
                                set {_kg} to random integer between 700 and 1200

                            chance of {@chance.3}:
                                set {_kg} to random integer between 700 and 1500

                            chance of {@chance.4}:
                                set {_kg} to random integer between 700 and 2000


                            if {_kg} >= 2000:
                                set {_g} to 0

                            drop 1 of {fishing.fish.참다랑어::1} with lore "" and "{@lt} &a크기: &e특대" and "{@lt} &d무게: &f%{_kg}%&f.&f%{_g}% &fkg" and "{@lt} &b어종: &f참다랑어" and "" and "&8[특별]" at {_loc}
                            broadcast "{@t} &b%event-player%&e님께서 &a참다랑어(특대)&e를 낚으셨습니다!"
                            send action bar "&a&l+ 참다랑어(특대)" to event-player


                            # 도감 기록 사용시 별도 작동
                            if {@use.collections} is true:
                                set yaml value "참다랑어_특대" from {_d} to "%{_kg}%.%{_g}%"
                                save yaml {_d}
                                unload yaml {_d}


                            stop

                        # 대 (400 ~ 699 kg)
                        chance of {@chance.12}:

                            set {_kg} to random integer between 400 and 450
                            set {_g} to random integer between 1 and 9


                            chance of {@chance.1}:
                                set {_kg} to random integer between 400 and 500

                            chance of {@chance.2}:
                                set {_kg} to random integer between 400 and 550

                            chance of {@chance.3}:
                                set {_kg} to random integer between 400 and 600

                            chance of {@chance.4}:
                                set {_kg} to random integer between 400 and 699


                            drop 1 of {fishing.fish.참다랑어::1} with lore "" and "{@lt} &a크기: &e대" and "{@lt} &d무게: &f%{_kg}%&f.&f%{_g}% &fkg" and "{@lt} &b어종: &f참다랑어" and "" and "&8[특별]" at {_loc}
                            broadcast "{@t} &b%event-player%&e님께서 &a참다랑어(대)&e를 낚으셨습니다!"
                            send action bar "&a&l+ 참다랑어(대)" to event-player


                            # 도감 기록 사용시 별도 작동
                            if {@use.collections} is true:
                                set yaml value "참다랑어_대" from {_d} to "%{_kg}%.%{_g}%"
                                save yaml {_d}
                                unload yaml {_d}


                            stop


                        send action bar "&a&l+ 참다랑어" to event-player
